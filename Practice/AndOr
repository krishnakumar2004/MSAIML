age = [15,21,45,55,60,70,9]

for i in age :
    if i > 18 and i < 50 :
        print(i,"go to road test")
    if i < 18 or i > 50 :
        print("person age is" ,i,"No road test")
print('bit wise operators -------------\n')
for i in age :
    if i > 18 & i < 50 :
        print(i,"go to road test")
    if i < 18 | i > 50 :
        print("person age is" ,i,"No road test")
